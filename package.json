{
  "name": "Dudung-Front",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "eslint-config-custom": "*",
    "prettier": "^2.8.1",
    "turbo": "latest",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@next/font": "13.1.1",
    "request": "^2.88.2",
    "yarn": "^1.22.19"
  },
  "dependenciesMeta": {
    "@emotion/react@11.10.5": {
      "unplugged": true
    }
  }
}
